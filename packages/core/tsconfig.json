{
  "compilerOptions": {
    "jsx": "react",
    "target": "es5",
    "module": "commonjs",
    "esModuleInterop": true,
    "outDir": "./lib",
    "skipLibCheck": true,
    "strict": true,
    "resolveJsonModule": true,
    "declaration": true,
    "isolatedModules": true,
    "moduleResolution": "node"
  },
  "include": ["src", "types/*"],
  "exclude": ["node_modules", "**/*.stories.tsx"]
}
